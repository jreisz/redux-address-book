{
  "name": "reactaddressbook",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://leanderlindahl.github.io/redux-address-book",
  "dependencies": {
    "axios": "^0.18.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "foundation-sites": "^6.5.0-rc.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-foundation": "^0.9.6",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.2",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "lint": "./node_modules/.bin/eslint ./src",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom --verbose=false",
    "test:coverage": "npm test -- --coverage",
    "test:update": "react-scripts test -u"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.0",
    "prettier": "^1.15.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  }
}
